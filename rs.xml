    <linkfile src="os_pickup_aosp.mk" dest="hardware/qcom/Android.mk" />
  </project>
  <project path="hardware/qcom-caf/msm8998/audio" name="aosp-playground/android_hardware_qcom-caf_msm8998_audio" revision="master" remote="github" />
